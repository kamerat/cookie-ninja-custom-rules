{
  "metadata": {
    "name": "Kamerat's nordic mixtape (cmp)",
    "version": "0.1.1",
    "description": "A set of mostly nordic rules used by Ninja Cookie (for cmp)",
    "homepage": "https://n0.no/",
    "bugs_email": "n0@n0.no",
    "license_file": "LICENSE"
  },
  "cmp/cookieinformation": {
    "metadata": {
      "name": "cookieinformation",
      "website": "https://cookieinformation.com/",
      "iab": "Cookieinformation.mgr.consensu.org"
    },

    "match": [
      { "type": "check", "selector": "#coiOverlay" }
    ],
    "action": [
      { "type": "hide" },
      { "type": "checkbox", "selector": "#cookie_cat_functional" },
      { "type": "checkbox", "selector": "#cookie_cat_statistic" },
      { "type": "checkbox", "selector": "#cookie_cat_marketing" },
      { "type": "sleep" },
      { "type": "click", "selector": "#declineButton" }
    ]
  }
}
